{
  "name": "bilibili-learning-list",
  "version": "0.0.1",
  "author": "YiguiDing",
  "description": "chrome-extension-bilibili-learning-list",
  "type": "module",
  "license": "LGPL-3.0-only",
  "keywords": [
    "chrome-extension",
    "vue",
    "vite",
    "create-chrome-ext"
  ],
  "engines": {
    "node": ">=14.18.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "fmt": "prettier --write '**/*.{vue,ts,json,css,scss,md}'",
    "prepare": "husky install",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "commit": "git cz"
  },
  "dependencies": {
    "@layui/layui-vue": "^2.12.0",
    "@vueuse/core": "^10.6.1",
    "axios": "^1.6.2",
    "chrome-types": "^0.1.246",
    "dayjs": "^1.11.10",
    "element-plus": "^2.4.2",
    "less": "^4.2.0",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@crxjs/vite-plugin": "^2.0.0-beta.19",
    "@types/node": "^20.9.5",
    "@vitejs/plugin-vue": "^4.4.0",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^8.0.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2",
    "vite": "^4.4.11",
    "vue-tsc": "^1.8.18"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
